# ZenTask Environment Configuration
# Copy this file to .env and fill in your values

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# Google Gemini API
GEMINI_API_KEY=your_gemini_api_key

# Jira Integration (Atlassian OAuth 2.0)
# Create an app at https://developer.atlassian.com/console/myapps/
ATLASSIAN_CLIENT_ID=your_atlassian_client_id
# Server-side only (used in Supabase Edge Functions, set via `supabase secrets set`)
ATLASSIAN_CLIENT_SECRET=your_atlassian_client_secret
